[Unit]
Description=uvicorn dev deamon
After=network.target

[Service]
User=natalia
Group=www-data
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=gunicorn_dev
WorkingDirectory=/opt/maia-dashboard-dev/backend
ExecStart=export ANALYSIS_DB='dev_analysis' &&
          export DASHBOARD_DB='dev_dashboard' &&
          export MONGODB_URL='mongodb://localhost:27017' &&
          /opt/maia-dashboard-dev/env/bin/uvicorn main:app --host 127.0.0.1 --port 7000

[Install]
WantedBy=multi-user.target
